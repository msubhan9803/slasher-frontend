{
  "name": "slasher-web-new",
  "version": "0.0.1",
  "description": "Slasher backend web app",
  "author": "Slasher Corp",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "NODE_ENV=development nest start",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:dev-https": "NODE_ENV=development HTTPS=true nest start --watch",
    "start:debug": "NODE_ENV=development nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --max-warnings=0",
    "find-floating-promises": "tslint --project tsconfig.json",
    "test": "jest --config ./test/jest-config.json --runInBand --detectOpenHandles",
    "test:watch": "jest --config ./test/jest-config.json --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "npm run test:e2e-core && npm run test:e2e-gateway",
    "test:e2e-core": "jest --config ./test/jest-e2e-core-config.json --runInBand",
    "test:e2e-gateway": "jest --config ./test/jest-e2e-gateway-config.json --runInBand"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.279.0",
    "@nestjs/axios": "^1.0.1",
    "@nestjs/bull": "^0.6.2",
    "@nestjs/common": "^9.2.1",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.2.1",
    "@nestjs/mongoose": "^9.2.1",
    "@nestjs/platform-express": "^9.2.1",
    "@nestjs/platform-socket.io": "^9.2.1",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/serve-static": "^3.0.0",
    "@nestjs/websockets": "^9.2.1",
    "@socket.io/redis-adapter": "^7.2.0",
    "@types/nodemailer": "^6.4.6",
    "@types/request-ip": "^0.0.37",
    "bcryptjs": "^2.4.3",
    "bull": "^4.10.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "email-validator": "^2.0.4",
    "express": "^4.18.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "luxon": "^3.2.1",
    "mime-types": "^2.1.35",
    "mongoose": "^6.7.2",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.7.8",
    "redis": "^4.5.0",
    "reflect-metadata": "^0.1.13",
    "request-ip": "^3.3.0",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "selfsigned": "^2.1.1",
    "socket.io": "^4.5.2",
    "socket.io-client": "^4.5.2",
    "uuid": "^9.0.0",
    "webpack": "^5.74.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@nestjs/cli": "^9.1.9",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.2.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.8",
    "@types/luxon": "^3.2.0",
    "@types/mime-types": "^2.1.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "eslint": "^8.23.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.0.4",
    "fishery": "^2.2.2",
    "follow-redirects": "^1.15.2",
    "jest": "^28.1.3",
    "source-map-support": "^0.5.20",
    "supertest": "^6.3.1",
    "ts-jest": "^28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^4.1.0",
    "tslint": "^6.1.3",
    "typescript": "^4.9.4"
  }
}
