  "scripts": {
    "build": "npm run sync-version-and-build-number && CI=false react-scripts build",
    "sync-version-and-build-number": "node ./scripts/sync-version-and-build-number.js",
    "analyze": "npm run build && source-map-explorer 'build/static/js/*.js'",
    "eject": "react-scripts eject",
    "lint": "eslint \"{src,e2e}/**/*.{ts,js,tsx, jsx}\" --max-warnings=0",
    "start": "react-scripts --max-old-space-size=8192 start",
    "start-https": "HTTPS=true react-scripts start",
    "serve-build": "serve -s build",
    "test": "react-scripts test --watchAll=false --transformIgnorePatterns 'node_modules/(?!swiper)/'",
    "test:e2e": "playwright test",
    "test:watch": "react-scripts test",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "cap-android": "cap sync android && cap run android",
    "cap-ios": "cap sync ios && cap run ios",
    "cap-run-android-dev-live-reload": "LIVE_RELOAD=true npm run cap-android",
    "cap-run-android-prod": "npm run build && npm run cap-android",
    "cap-run-ios-dev-live-reload": "LIVE_RELOAD=true npm run cap-ios",
    "cap-run-ios-prod": "npm run build && npm run cap-ios"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
